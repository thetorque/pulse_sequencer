--synthesis_resources = lut 108 
SUBDESIGN cmpr_ach
( 
	aeb	:	output;
	agb	:	output;
	alb	:	output;
	dataa[63..0]	:	input;
	datab[63..0]	:	input;
) 
VARIABLE
	aeb_int	:	WIRE;
	alb_int	:	WIRE;
BEGIN 
	IF (dataa[] == datab[]) THEN
		aeb_int = VCC;
	ELSE
		aeb_int = GND;
	END IF;
	IF (dataa[] < datab[]) THEN
		alb_int = VCC;
	ELSE
		alb_int = GND;
	END IF;
	aeb = aeb_int;
	alb = alb_int;
	agb = !alb_int & !aeb_int;
END;
